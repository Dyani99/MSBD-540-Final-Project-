      "source": [
        "# Import libraries\n",
        "import pandas as pd\n",
        "import numpy as np"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Define random seed\n",
        "np.random.seed(42)"
      ],
      "metadata": {
        "id": "BdFrNTWhrxBB"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Define genes and mutation types\n",
        "genes = ['TPM2', 'PIEZO2', 'TNNT3']\n",
        "mutation_types = ['Missense', 'Nonsense', 'Frameshift']\n",
        "\n",
        "# Define the size of the dataset\n",
        "n = 500"
      ],
      "metadata": {
        "id": "l_oKiZwcsiTE"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Create the dataframe
synthetic_df = pd.DataFrame({
    'Gene': np.random.choice(genes, size=n, p=[0.3, 0.5, 0.2]),
    'MutationType' : np.random.choice(mutation_types, size=n),
    'PolyPhen2_Score' : np.round(np.random.beta(5, 2, size=n), 2),
    'SIFT_Score' : np.round(np.random.beta(2, 5, size=n), 2),
    'Conservation_Score' : np.round(np.random.uniform(0.6, 1.0, size=n), 2),
    'AlleleFrequency' : np.round(np.random.exponential(0.001, size=n), 5)
})

# Mobility severity
synthetic_df['MobilitySeverity'] = ((synthetic_df['Gene'] == 'PIEZO2') &
                          (synthetic_df['PolyPhen2_Score'] > 0.8) &
                          (synthetic_df['SIFT_Score'] < 0.05)).astype(int)

# Add Source column for traceability
synthetic_df['Source'] = 'Synthetic Data'

# Add two real mutations from Liu et al. (2018)
real_data = [
    {
        "Gene": "TPM2",
        "MutationType": "Missense",
        "PolyPhen2_Score": 0.96,
        "SIFT_Score": 0.01,
        "Conservation_Score": 0.95,
        "AlleleFrequency": 0.00001,
        "MobilitySeverity": 1,
        "Source": "Liu2018"
    },
    {
        "Gene": "PIEZO2",
        "MutationType": "Missense",
        "PolyPhen2_Score": 0.88,
        "SIFT_Score": 0.06,
        "Conservation_Score": 0.91,
        "AlleleFrequency": 0.00002,
        "MobilitySeverity": 0,
        "Source": "Liu2018"
    }
]

real_df = pd.DataFrame(real_data)

# Combine datasets
final_df = pd.concat([real_df, synthetic_df], ignore_index=True)

final_df.head()
